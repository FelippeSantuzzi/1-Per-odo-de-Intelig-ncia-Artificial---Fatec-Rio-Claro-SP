      "source": [
        "# Vamos tentar identificar as colunas que representam opções de resposta\n",
        "# e calcular a porcentagem de 'X' em cada uma delas.\n",
        "\n",
        "print(\"Análise de Porcentagem de Escolha por Opção:\")\n",
        "\n",
        "# Iterar pelas colunas do DataFrame\n",
        "for col in df_planilha.columns:\n",
        "    # Verificar se a coluna contém 'X' e tem mais de um valor não nulo (para evitar colunas de identificação)\n",
        "    if 'X' in df_planilha[col].unique() and df_planilha[col].count() > 1:\n",
        "        print(f\"\\nAnálise para a coluna: '{col}'\")\n",
        "\n",
        "        # Contar os valores não nulos (assumindo que 'X' é a escolha)\n",
        "        total_respostas = df_planilha[col].count()\n",
        "\n",
        "        if total_respostas > 0:\n",
        "            # Contar quantas vezes 'X' aparece\n",
        "            contagem_x = df_planilha[col].eq('X').sum()\n",
        "\n",
        "            # Calcular a porcentagem de 'X'\n",
        "            porcentagem_x = (contagem_x / total_respostas) * 100\n",
        "\n",
        "            print(f\"  Total de respostas nesta coluna (não nulo): {total_respostas}\")\n",
        "            print(f\"  Contagem de 'X': {contagem_x}\")\n",
        "            print(f\"  Porcentagem de escolha ('X'): {porcentagem_x:.2f}%\")\n",
        "        else:\n",
        "            print(\"  Nenhuma resposta encontrada nesta coluna.\")\n",
        "\n",
        "    # Se a coluna principal de pergunta tem valores como 'Uso doméstico', 'Uso profissional', etc.\n",
        "    # Podemos analisar a distribuição desses valores também.\n",
        "    # Vamos verificar algumas colunas principais conhecidas:\n",
        "    if col in ['Para que finalidade pretende usar o moedor?', 'Quantos quilos de carne precisa moer?', 'Qual é a voltagem disponível no local?']:\n",
        "         print(f\"\\nDistribuição de respostas para a pergunta: '{col}'\")\n",
        "         # Calcular a contagem de cada valor único e normalizar para obter porcentagens\n",
        "         distribuicao_porcentagem = df_planilha[col].value_counts(normalize=True) * 100\n",
        "         display(distribuicao_porcentagem.round(2)) # Exibir com 2 casas decimais"
      ],
